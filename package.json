{
  "name": "web-dapp-sample",
  "version": "1.0.0",
  "description": "Basic demo for using user auth in Dash dapp",
  "main": "app/server.js",
  "scripts": {
    "test": "DEBUG=server:debug cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles",
    "test:watch": "DEBUG=server:debug cross-env NODE_ENV=test jest --watch",
    "test-unit:watch": "DEBUG=server:debug cross-env NODE_ENV=test jest ./tests/unit --watch",
    "test-routes:watch": "DEBUG=server:debug cross-env NODE_ENV=test jest ./tests/routes --watch",
    "test-services:watch": "DEBUG=server:debug cross-env NODE_ENV=test jest ./tests/services --watch --runInBand --detectOpenHandles",
    "start": "node -r esm app/server.js",
    "develop": "DEBUG=server:debug cross-env NODE_ENV=development nodemon -r esm app/server.js",
    "precommit": "pretty-quick --staged",
    "postcommit": "git update-index -g",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "lint": "eslint . --ext .js",
    "docs-readme:models": "documentation readme app/models/** --section=Models",
    "docs-readme:services": "documentation readme app/services/** --section=Services",
    "client-start": "cd app/static && webpack-dev-server",
    "client-build": "cd app/static && webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashmachine/web-dapp-sample.git"
  },
  "keywords": [
    "Dash",
    "Dash Platform",
    "DApp"
  ],
  "author": "Nigel Wheeler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dashmachine/web-dapp-sample/issues"
  },
  "homepage": "https://github.com/dashmachine/web-dapp-sample#readme",
  "dependencies": {
    "bitcore-ecies-dash": "^1.0.4",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.4",
    "dash": "^3.0.2",
    "dashmachine-auth-request": "^0.3.1",
    "dashmachine-crypto": "^0.4.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.3",
    "express-session": "^1.17.1",
    "memorystore": "^1.6.2",
    "redis": "^3.0.2",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@material/auto-init": "^6.0.0",
    "@material/button": "^6.0.0",
    "@material/card": "^6.0.0",
    "@material/icon-button": "^6.0.0",
    "@material/layout-grid": "^6.0.0",
    "@material/linear-progress": "^6.0.0",
    "@material/ripple": "^6.0.0",
    "@material/snackbar": "^6.0.0",
    "@material/tab-bar": "^6.0.0",
    "@material/textfield": "^6.0.0",
    "@material/theme": "^6.0.0",
    "@material/top-app-bar": "^6.0.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "extract-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "redis-mock": "^0.49.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
